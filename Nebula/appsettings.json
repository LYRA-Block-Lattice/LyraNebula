{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "network": "mainnet",
  "localstore": "yes",
  "Lyra": {
    "Mongodb": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "%USERPROFILE%/.lyra/mainnet/logs/nebula-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "ipdb": "%USERPROFILE%\\.lyra\\res\\ip2country.zip",
  "proxy": "http://192.168.3.101:1085"
}
